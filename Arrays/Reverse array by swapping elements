
*** To reverse array by swapping elements***
*** Time complexity : O(N)
*** Space complexity : 0(1)

#include<iostream>
using namespace std;
int main()
{
  int arr[]={9,3,1,7,95,3,984,156};

   for(int i=0,j=(sizeof(arr)/4)-1;i<(sizeof(arr)/4)/2;i++,j--)             
    swap(arr[i],arr[j]); 
  
   for(int i=0;i<(sizeof(arr)/4);i++)
    cout<<arr[i]<<" ";
}
